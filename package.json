{
  "name": "@medyll/slotui",
  "version": "0.4.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/medyll/slot-ui.git"
  },
  "scripts": {
    "dev": "vite dev  --port 3001 --host --open   ",
    "build": "svelte-kit sync && vite build",
    "package": "svelte-kit sync && svelte-package",
    "package-watch": "svelte-kit sync && svelte-package -w",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "make-docs": "node build-docs & exit 0",
    "make-docs-watch": "nodemon -e demo.svelte,preview.svelte -x \"npm run make-docs\"",
    "ensure-defs": "node ensureDefs & exit 0",
    "ensure-defs-watch": "nodemon -e .svelte -x \"npm run ensure-defs\"",
    "css-build": "sass sass/cssfabric.scss static/cssfabric/cssfabric.vars.css",
    "css-watch": "npm run css-build -- --watch",
    "precommit": "npm unlink @medyll/cssfabric & npm i @medyll/cssfabric ",
    "postcommit": "npm remove @medyll/cssfabric & npm link --save @medyll/cssfabric",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "powerShell": "Start-process powershell -Verb runAs",
    "test": "jest"
  },
  "type": "module",
  "dependencies": {
    "@medyll/cssfabric": "^0.2.6",
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "@sveltestack/svelte-query": "^1.6.0",
    "d3": "^7.4.4",
    "iconify-icon": "^1.0.0",
    "lodash": "^4.17.21",
    "postcss-load-config": "^4.0.1",
    "prismjs": "^1.29.0",
    "sanitize-html": "^2.7.2",
    "svelte-markdown": "^0.2.2",
    "svelte-spa-router": "^3.2.0",
    "svelte-use-form": "^2.4.3",
    "svelte2tsx": "^0.5.11"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-node": "next",
    "@sveltejs/adapter-static": "next",
    "@sveltejs/adapter-vercel": "1.0.0-next.75",
    "@sveltejs/kit": "1.0.0-next.511",
    "@sveltejs/package": "1.0.0-next.5",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/svelte": "^3.1.0",
    "@testing-library/user-event": "^14.0.4",
    "@types/color": "^3.0.3",
    "@types/d3": "^7.4.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.178",
    "@types/prismjs": "^1.26.0",
    "@types/sanitize-html": "^2.6.2",
    "axios": "^0.27.2",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "jest": "^27.5.1",
    "mdsvex": "^0.10.6",
    "nodemon": "^2.0.18",
    "prism-svelte": "^0.5.0",
    "svelte": "^3.50.1",
    "svelte-check": "^2.8.0",
    "svelte-jester": "^2.3.2",
    "svelte-loader": "^3.1.3",
    "svelte-preprocess": "^4.10.7",
    "svelte-preprocess-markdown": "^2.7.3",
    "ts-jest": "^27.1.4",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.1.0"
  }
}
